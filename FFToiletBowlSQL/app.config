<?xml version="1.0"?>
<configuration>
  <startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <connectionStrings>
    <!--https://stackoverflow.com/questions/3500829/sql-express-connection-string-mdf-file-location-relative-to-application-locatio-->
    <add name="LocalDB"
          connectionString="Data Source=(LocalDb)\MSSQLLocalDB;Initial Catalog=FFToiletBowl;Integrated Security=SSPI;AttachDBFilename=|DataDirectory|\FFToiletBowl.mdf"
          providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
      <add key="Database" value="LocalDB"/>
  </appSettings>
</configuration>
